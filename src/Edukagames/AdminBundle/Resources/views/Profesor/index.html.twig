{% extends '::index.html.twig' %}
{% block content %}
<h1>Lista de Administradores</h1>
<i class="icon-plus"></i><a href="{{ path('profesor_new') }}">Nuevo Administrador</a>
<table class="table table-striped table-condensed" cellspacing="0">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><i class="icon-user"></i> {{ entity.nombre }}</td>
            <td>
{#             	<a href="{{ path('profesor_show', { 'id': entity.id }) }}"><i class="icon-eye-open"></i></a>#}
				<a href="{{ path('profesor_edit', { 'id': entity.id }) }}"><i class="icon-pencil"></i></a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>


{% endblock %}